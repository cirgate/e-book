.left_arrow {
  width: 50rpx;
  height: 50rpx;
  border-top: 2px solid #198331;
  border-right: 2px solid #198331;
  position: absolute;
  left: 50rpx;
  transform: rotate(220deg);
  bottom: 20px;
}

.arrow {
  width: 50rpx;
  height: 50rpx;
  border-top: 2px solid #198331;
  border-right: 2px solid #198331;
  position: absolute;
  right: 50rpx;
  transform: rotate(45deg);
  bottom: 20px;
}

.icon_input {
  position: fixed;
  bottom: 20px;
  margin: 20rpx;
}

